{
  "$schema": "https://turbo.build/schema.json",
  "extends": [
    "//"
  ],
  "pipeline": {
    "copy:assets": {
      "cache": false
    },
    "build": {
      "dependsOn": [
        "copy:assets",
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "dev": {
      "dependsOn": [
        "^build"
      ]
    }
  }
}